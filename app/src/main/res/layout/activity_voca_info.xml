<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".vocagroup.VocagroupAdd">



    <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#4a4646"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/Toolbar_vocaInfo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"/>
    <ImageButton
            android:contentDescription="@string/goback"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/abc_vector_test"
            android:id="@+id/ImageButton_vocaInfo_goback"
            tools:layout_editor_absoluteY="116dp"
            app:layout_constraintStart_toStartOf="@+id/Toolbar_vocaInfo"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintTop_toTopOf="@+id/Toolbar_vocaInfo"
            app:layout_constraintBottom_toBottomOf="@+id/Toolbar_vocaInfo"/>
    <TextView
            android:text="@string/titlevocainfo"
            android:gravity="center"
            android:textSize="18sp"
            android:textColor="#ffffff"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/ImageButton_vocaInfo_title"
            app:layout_constraintStart_toStartOf="@+id/Toolbar_vocaInfo"
            app:layout_constraintEnd_toEndOf="@+id/Toolbar_vocaInfo"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintTop_toTopOf="@+id/Toolbar_vocaInfo" android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:layout_marginRight="16dp" android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/Toolbar_vocaInfo" android:layout_marginBottom="16dp"/>
    <Button
            android:text="@string/modify"
            android:textColor="#000000"
            android:textSize="20sp"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:background="@drawable/buttonshape"
            android:id="@+id/Button_vocaInfo_add"
            app:layout_constraintEnd_toEndOf="@+id/Toolbar_vocaInfo"
            android:layout_marginEnd="8dp"
            app:layout_constraintTop_toTopOf="@id/Toolbar_vocaInfo"
            app:layout_constraintBottom_toBottomOf="@+id/Toolbar_vocaInfo"/>
    <View
            android:id="@+id/divider_vocaInfo_left"
            android:layout_width="1dp"
            android:layout_height="729dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="24dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="1dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="1dp"/>
    <View
            android:id="@+id/divider_vocaInfo_right"
            android:layout_width="1dp"
            android:layout_height="729dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="24dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="1dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="1dp"/>


    <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/divider_vocaInfo_left"
            app:layout_constraintEnd_toEndOf="@id/divider_vocaInfo_right"
            app:layout_constraintTop_toBottomOf="@+id/Toolbar_vocaInfo" android:layout_marginTop="1dp"
            android:layout_marginStart="1dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="1dp" android:layout_marginBottom="1dp">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical">
            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <TextView
                        android:text="@string/vocagroupselect"
                        android:textSize="18sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/TextView_vocaInfo_vocagroupName"
                        app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="50dp"
                        app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        tools:text="@string/vocagroupexample"
                        android:textSize="18sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocaInfo_vocagroupNameInput"
                        app:layout_constraintTop_toTopOf="@+id/TextView_vocaInfo_vocagroupName"
                        app:layout_constraintStart_toEndOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginStart="5dp"/>

                <TextView
                        android:text="@string/currentlearningcycle"
                        android:textSize="18sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/TextView_vocaInfo_currentlearningcycle"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginTop="50dp"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocaInfo_vocagroupName"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:text=""
                        android:textSize="18sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocaInfo_currentlearningcycleNumber"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocaInfo_currentlearningcycle"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginTop="8dp"/>

                <TextView
                        android:text="@string/vocagrouparea"
                        android:textSize="20sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/TextView_vocaInfo_vocagroupArea1"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginTop="50dp"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocaInfo_currentlearningcycleNumber"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:text=""
                        android:textSize="20sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocaInfo_vocagroupArea1Input"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocaInfo_vocagroupArea1"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginTop="8dp"/>
                <TextView
                        android:text="@string/vocagrouparea"
                        android:textSize="20sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/TextView_vocaInfo_vocagroupArea2"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocaInfo_vocagroupArea1Input"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginTop="24dp"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:text=""
                        android:textSize="20sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocaInfo_vocagroupArea2Input"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocaInfo_vocagroupArea2"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginTop="8dp"/>
                <TextView
                        android:text="@string/vocagrouparea"
                        android:textSize="20sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/TextView_vocaInfo_vocagroupArea3"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocaInfo_vocagroupArea2Input"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginTop="24dp"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:text=""
                        android:textSize="20sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocaInfo_vocagroupArea3Input"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocaInfo_vocagroupArea3"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocaInfo_vocagroupName"
                        android:layout_marginTop="8dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>