<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".vocagroup.VocagroupAdd">



    <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#4a4646"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/Toolbar_vocagroupInfo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"/>
    <ImageButton
            android:contentDescription="@string/goback"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/abc_vector_test"
            android:id="@+id/ImageButton_vocagroupInfo_goback"
            tools:layout_editor_absoluteY="116dp"
            app:layout_constraintStart_toStartOf="@+id/Toolbar_vocagroupInfo"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintTop_toTopOf="@+id/Toolbar_vocagroupInfo"
            app:layout_constraintBottom_toBottomOf="@+id/Toolbar_vocagroupInfo"/>
    <TextView
            android:text="@string/titlevocagroupinfo"
            android:gravity="center"
            android:textSize="18sp"
            android:textColor="#ffffff"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/ImageButton_vocagroupInfo_title"
            app:layout_constraintStart_toStartOf="@+id/Toolbar_vocagroupInfo"
            app:layout_constraintEnd_toEndOf="@+id/Toolbar_vocagroupInfo"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintTop_toTopOf="@+id/Toolbar_vocagroupInfo" android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:layout_marginRight="16dp" android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/Toolbar_vocagroupInfo" android:layout_marginBottom="16dp"/>
    <Button
            android:text="@string/modify"
            android:textColor="#000000"
            android:textSize="20sp"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:background="@drawable/buttonshape"
            android:id="@+id/Button_vocagroupInfo_add"
            app:layout_constraintEnd_toEndOf="@+id/Toolbar_vocagroupInfo"
            android:layout_marginEnd="8dp"
            app:layout_constraintTop_toTopOf="@id/Toolbar_vocagroupInfo"
            app:layout_constraintBottom_toBottomOf="@+id/Toolbar_vocagroupInfo"/>
    <View
            android:id="@+id/divider_vocagroupInfo_left"
            android:layout_width="1dp"
            android:layout_height="729dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="24dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="1dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="1dp"/>
    <View
            android:id="@+id/divider_vocagroupInfo_right"
            android:layout_width="1dp"
            android:layout_height="729dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="24dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="1dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="1dp"/>


    <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/divider_vocagroupInfo_left"
            app:layout_constraintEnd_toEndOf="@id/divider_vocagroupInfo_right"
            app:layout_constraintTop_toBottomOf="@+id/Toolbar_vocagroupInfo" android:layout_marginTop="1dp"
            android:layout_marginStart="1dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="1dp" android:layout_marginBottom="1dp">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical">
            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <TextView
                        android:text="@string/totalvocacount"
                        android:textSize="20sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/TextView_vocagroupInfo_totalVocaCount"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="50dp"/>
                <TextView
                        tools:text="10000(임시)"
                        android:textSize="20sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/TextView_vocagroupInfo_totalVocaNumber"
                        app:layout_constraintTop_toTopOf="@+id/TextView_vocagroupInfo_totalVocaCount"
                        app:layout_constraintStart_toEndOf="@+id/TextView_vocagroupInfo_totalVocaCount"
                        android:layout_marginStart="24dp"/>
                <TextView
                        android:text="@string/vocagroupname"
                        android:textSize="20sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/TextView_vocagroupInfo_vocagroupName"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_totalVocaCount"
                        android:layout_marginTop="24dp"
                        app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:text=""
                        android:textSize="20sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocagroupInfo_vocagroupNameInput"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_vocagroupName"
                        android:layout_marginTop="8dp"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocagroupInfo_vocagroupName"/>

                <TextView
                        android:text="@string/vocagroupcycle"
                        android:textSize="20sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/TextView_vocagroupInfo_vocagroupCycle"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_vocagroupNameInput"
                        android:layout_marginTop="24dp"
                        app:layout_constraintStart_toStartOf="parent"/>
                <Spinner
                        android:background="@drawable/spinner_background"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:id="@+id/Spinner_vocagroupInfo_vocagroupCycle"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_vocagroupCycle"
                        app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                        android:text="@string/vocagrouparea"
                        android:textSize="20sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/TextView_vocagroupInfo_vocagroupArea1"
                        app:layout_constraintTop_toBottomOf="@+id/Spinner_vocagroupInfo_vocagroupCycle"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocagroupInfo_vocagroupName"
                        android:layout_marginTop="24dp"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:text=""
                        android:textSize="20sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocagroupInfo_vocagroupArea1Input"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_vocagroupArea1"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocagroupInfo_vocagroupName"
                        android:layout_marginTop="8dp"/>
                <Switch
                        android:text="@string/arealocation"
                        android:textSize="15sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/switch1"
                        android:thumb="@drawable/switch_thumb_selector"
                        android:track="@drawable/switch_track"
                        android:switchPadding="15dp"
                        android:showText="true"
                        android:textOff="@string/frontside"
                        android:textOn="@string/backside"
                        app:layout_constraintStart_toEndOf="@id/TextView_vocagroupInfo_vocagroupArea1Input"
                        app:layout_constraintBaseline_toBaselineOf="@+id/TextView_vocagroupInfo_vocagroupArea1Input"
                        android:layout_marginStart="15dp"/>
                <TextView
                        android:text="@string/vocagrouparea"
                        android:textSize="20sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/TextView_vocagroupInfo_vocagroupArea2"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_vocagroupArea1Input"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocagroupInfo_vocagroupName"
                        android:layout_marginTop="24dp"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:text=""
                        android:textSize="20sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocagroupInfo_vocagroupArea2Input"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_vocagroupArea2"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocagroupInfo_vocagroupName"
                        android:layout_marginTop="8dp"/>
                <Switch
                        android:text="@string/arealocation"
                        android:textSize="15sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/switch2"
                        android:thumb="@drawable/switch_thumb_selector"
                        android:track="@drawable/switch_track"
                        android:switchPadding="15dp"
                        android:showText="true"
                        android:textOff="@string/frontside"
                        android:textOn="@string/backside"
                        app:layout_constraintStart_toEndOf="@id/TextView_vocagroupInfo_vocagroupArea2Input"
                        app:layout_constraintBaseline_toBaselineOf="@+id/TextView_vocagroupInfo_vocagroupArea2Input"
                        android:layout_marginStart="15dp"/>
                <TextView
                        android:text="@string/vocagrouparea"
                        android:textSize="20sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/TextView_vocagroupInfo_vocagroupArea3"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_vocagroupArea2Input"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocagroupInfo_vocagroupName"
                        android:layout_marginTop="24dp"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:text=""
                        android:textSize="20sp"
                        android:ems="10"
                        android:id="@+id/TextView_vocagroupInfo_vocagroupArea3Input"
                        app:layout_constraintTop_toBottomOf="@+id/TextView_vocagroupInfo_vocagroupArea3"
                        app:layout_constraintStart_toStartOf="@+id/TextView_vocagroupInfo_vocagroupName"
                        android:layout_marginTop="8dp"/>
                <Switch
                        android:text="@string/arealocation"
                        android:textSize="15sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/switch3"
                        android:thumb="@drawable/switch_thumb_selector"
                        android:track="@drawable/switch_track"
                        android:switchPadding="15dp"
                        android:showText="true"
                        android:textOff="@string/frontside"
                        android:textOn="@string/backside"
                        app:layout_constraintStart_toEndOf="@id/TextView_vocagroupInfo_vocagroupArea3Input"
                        app:layout_constraintBaseline_toBaselineOf="@+id/TextView_vocagroupInfo_vocagroupArea3Input"
                        android:layout_marginStart="15dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>